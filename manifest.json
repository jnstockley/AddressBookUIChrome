  "name": "AddressBook UI",
  "short_name": "AddressBook",
  "version": "2.00",
  "description": "Create, Read, Update, and Destroy data from the MySQL Address Book Server",
  "permissions": ["declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
